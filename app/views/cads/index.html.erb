<p id="notice"><%= notice %></p>

<h1>Cadastro</h1>

<table>
  <thead>
    <tr>
      <th>Nomee</th>
      <th>Cnpjcpf</th>
      <th>Ie</th>
      <th>Celular</th>
      <th>Telefone</th>
      <th>Nomecont</th>
      <th>Dtnasc</th>
      <th>Celular2</th>
      <th>Telefone2</th>
      <th>Nomecont2</th>
      <th>Email</th>
      <th>Cep</th>
      <th>Rua</th>
      <th>Numcasa</th>
      <th>Cidade</th>
      <th>Bairro</th>
      <th>Estado</th>
      <th>Pais</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cads.each do |cad| %>
      <tr>
        <td><%= cad.nome %></td>
        <td><%= cad.cnpjcpf %></td>
        <td><%= cad.ie %></td>
        <td><%= cad.celular %></td>
        <td><%= cad.telefone %></td>
        <td><%= cad.nomecont %></td>
        <td><%= cad.dtnasc %></td>
        <td><%= cad.celular2 %></td>
        <td><%= cad.telefone2 %></td>
        <td><%= cad.nomecont2 %></td>
        <td><%= cad.email %></td>
        <td><%= cad.cep %></td>
        <td><%= cad.rua %></td>
        <td><%= cad.numcasa %></td>
        <td><%= cad.cidade %></td>
        <td><%= cad.bairro %></td>
        <td><%= cad.estado %></td>
        <td><%= cad.pais %></td>
        <td><%= link_to 'Mostrar', cad %></td>
        <td><%= link_to 'Editar', edit_cad_path(cad) %></td>
        <td><%= link_to 'Excluir', cad, method: :delete, data: { confirm: 'VocÃª tem certeza que deseja excluir?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo Cadastro', new_cad_path %>
